<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Edit Analytic Account in Move Lines -->
    <record id="account_move_line_edit_analytic_accounts_view" model="ir.ui.view">
        <field name="name">account.move.line.edit.analytic.accounts</field>
        <field name="model">account.move.line</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="company_id" invisible="1"/>
                <field name="move_id" readonly="1"/>
                <field name="analytic_account_id"/>
            </tree>
        </field>
    </record>

</odoo>